@model IEnumerable<IntelligenceAgencyManagementSystem.Models.User>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Список користувачів</h2>

<table class="table">
    <thead>
    <tr>
        <td>Email</td>
        <td>Рік народження</td>
        <td></td>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>@user.Year.ToString()</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userId="@user.Id">Права доступу</a>
            </td>
        </tr>
    }
    </tbody>
</table>



